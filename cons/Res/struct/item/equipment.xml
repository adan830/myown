	<properties>
        <property name="EquipType" type="string" public="false" private="false" save="false" desc="装备栏位类型, equipment_config.xml文件中的定义"/>
		<property name="BasePackage" type="int" public="false" private="false" save="false" desc="基础属性包"/>
		<property name="StrPackage" type="int" public="false" private="false" save="false" desc="强化基础包"/>
