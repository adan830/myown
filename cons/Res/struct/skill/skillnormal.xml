		<property name="CurHits" type="int" public="false" private="false" save="false" desc="技能当前打击次数"/>
		<property name="CoolDownTime" type="int" public="false" private="false" save="false" desc="冷却时间,单位为ms"/>
		<property name="SkillAttackAdd" type="int" public="false" private="false" save="false" realtime="false" desc="技能攻击力"/>
		<property name="SkillDamageRate" type="float" public="false" private="false" save="false" realtime="false" desc="技能伤害倍率"/>
		<property name="SkillFormula" type="float" public="false" private="false" save="false" realtime="false" desc="技能公式结算的值"/>
	</properties>
</object>
